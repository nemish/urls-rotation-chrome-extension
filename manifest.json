{
    "manifest_version": 3,
    "name": "Rotatio Extension",
    "description": "Rotates between a list of urls",
    "version": "1.0",
    "action": {
       "default_title": "Start urls rotation",
       "default_icon": "rotatio.png",
       "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting", "tabs", "storage"]
  }